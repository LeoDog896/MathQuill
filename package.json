{
  "name": "mathquill",
  "description": "Easily type math in your webapp",
  "version": "0.10.1",
  "type": "module",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mathquill/mathquill.git"
  },
  "files": [
    "build/mathquill.{css,js,min.js}",
    "build/fonts/Symbola.*",
    "quickstart.html"
  ],
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^17.0.32",
    "@types/uglify-js": "^3.13.2",
    "chalk": "^5.0.1",
    "husky": "^7.0.4",
    "less": ">=1.5.1 <3.0.0",
    "lint-staged": "^12.3.1",
    "mocha": ">=2.4.1",
    "prettier": "^2.5.1",
    "terser": "^5.13.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.2"
  },
  "lint-staged": {
    "*.{ts,js,css,html}": "prettier --write"
  },
  "scripts": {
    "prepare": "husky install",
    "prettify": "npx prettier --write '**/*.{ts,js,css,html}'",
    "server": "tsc -p scripts.tsconfig.json && node script/dist/test_server.js",
    "lint": "npx tsc --noEmit && npx tsc --noEmit -p test/tsconfig.public-types-test.json"
  }
}
